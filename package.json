{
  "name": "attajak-hugo-doks",
  "version": "0.1.0",
  "description": "Hugo + Doks theme",
  "author": "attajak (https://github.com/attajak)",
  "license": "MIT",
  "scripts": {
    "create": "hugo new content",
    "dev": "hugo server -D -E -F -N -O --disableFastRender --noHTTPCache --renderToMemory --logLevel info -b http://localhost:1313/",
    "format": "prettier **/** -w -c",
    "build": "hugo --gc --minify --logLevel info",
    "preview": "vite preview --outDir public"
  },
  "dependencies": {
    "@tabler/icons": "^3.30.0",
    "@thulite/doks-core": "^1.8.1",
    "@thulite/images": "^3.3.0",
    "@thulite/inline-svg": "^1.2.0",
    "@thulite/seo": "^2.4.1",
    "thulite": "^2.5.0"
  },
  "devDependencies": {
    "prettier": "^3.5.0",
    "vite": "^7.0.2"
  },
  "engines": {
    "node": ">=20.11.0"
  }
}
